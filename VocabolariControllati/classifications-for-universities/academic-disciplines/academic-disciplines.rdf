    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-MED-27"/>
    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines"/>
