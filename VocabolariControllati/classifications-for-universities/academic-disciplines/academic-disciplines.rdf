  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SC-09-A1">
    <skos:broader rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/MSC-09-A"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
