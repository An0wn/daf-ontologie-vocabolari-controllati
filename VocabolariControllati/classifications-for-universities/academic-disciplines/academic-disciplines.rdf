  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SC-06-D4">
    <skos:topConceptOf rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines"/>
    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-MED-35"/>
