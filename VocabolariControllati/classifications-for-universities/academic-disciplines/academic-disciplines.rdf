    <skos:prefLabel xml:lang="it">Settore concorsuale: Storia contemporanea</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SC-09-F1">
    <skos:notation>09/F1</skos:notation>
