  <rdf:Description rdf:about="http://dati.gov.it/onto/controlledvocabulary/POICategoryClassification/cat_8_publicservice">
    <rdf:type rdf:resource="http://dati.gov.it/onto/poiapit#PointOfInterestCategory"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="it">Settore altri servizi pubblici</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Other public services</skos:prefLabel>
    <poiapit:POIcategoryName xml:lang="it">Settore altri servizi pubblici</poiapit:POIcategoryName>
    <dct:identifier>cat_8</dct:identifier>
