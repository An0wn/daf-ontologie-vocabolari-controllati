  <rdf:Description rdf:about="http://dati.gov.it/onto/controlledvocabulary/POICategoryClassification/cat_6_travel">
    <skos:inScheme rdf:resource="http://dati.gov.it/onto/controlledvocabulary/POICategoryClassification"/>
    <poiapit:POIcategoryName xml:lang="en">Travel/Tourism</poiapit:POIcategoryName>
    <skos:definition xml:lang="it">Rientrano in questa categoria tutti i punti di interesse connessi al settore dei viaggi e del turismo quali per esempio agenzie viaggi, strutture ricettive, centri di cambio valuta, info point per turisti, luoghi della cultura, luoghi freschi, ecc.</skos:definition>
    <dct:identifier>cat_6</dct:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="it">Settore viaggi/turismo</skos:prefLabel>
