  <rdf:Description rdf:about="http://dati.gov.it/onto/controlledvocabulary/Authentication/IDPWD">
    <skos:prefLabel xml:lang="en">User ID and password</skos:prefLabel>
    <skos:notation>IDPWD</skos:notation>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Simple credential consisting in username and password. Access to online Italian systems such as NoiPA and Fiscoonline and any other system that requires username and password fall into this category</skos:definition>
    <skos:inScheme rdf:resource="http://dati.gov.it/onto/controlledvocabulary/Authentication"/>
    <skos:prefLabel xml:lang="it">User ID e password</skos:prefLabel>
    <dct:identifier>IDPWD</dct:identifier>
    <skos:definition xml:lang="it">Credenziale semplice basata sull'uso di username e password. Accesso tramite sistemi quali NoiPA, FiscoOnline e ogni altro sistema che richiede solo credenziali di tipo usernname e password ricadono in questa categoria</skos:definition>
    <skos:broader rdf:resource="http://dati.gov.it/onto/controlledvocabulary/Authentication/SFA"/>
    <clvapit:hasRankOrder>2</clvapit:hasRankOrder>
  </rdf:Description>
