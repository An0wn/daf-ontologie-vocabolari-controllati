        {
          "@language": "it",
          "@value": "stabile"
        },
        "stable"
      ],
      "rdfs:comment": [
        {
          "@language": "en",
          "@value": "It represents the information on the temporary closure "
        },
        {
          "@language": "it",
          "@value": "Rappresenta le informazioni sulla chiusura temporanea"
        }
