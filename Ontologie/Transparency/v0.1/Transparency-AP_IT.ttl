###  https://w3id.org/italia/onto/Transparency/TransparencyActivity
:TransparencyActivity a owl:Class ;
	rdfs:label "Transparency activity"@en, "Attività di trasparenza."@it;
	rdfs:comment "This class represents a trasparency activity as an effect of a transparency obbligation. For example pubblication, update,..."@en,
				 "Questa classe rappresenta un'attività di trasparenza come conseguenza di un obbligo di trasparenza."@it;
	rdfs:subClassOf <https://w3id.org/italia/onto/l0/Activity> ,  
    	[ a owl:Restriction ;
    		owl:onProperty :generatesResource ;
    		owl:someValuesFrom :TransparencyResource
    	] ,
    	[ a owl:Restriction ;
    		owl:onProperty tiapit:date ;
    		owl:cardinality "1"^^xsd:nonNegativeInteger;
			owl:onDataRange [ owl:unionOf ( xsd:date
                                 		  xsd:dateTime
                              			  )
                 			] ;
    	],
		[ a owl:Restriction ;
    		owl:onProperty :isPerformedBy ;
    		owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger;
			owl:onClass l0:Agent; 
    	],
		[ a owl:Restriction ;
			owl:onProperty :hasTransparencyActivityTypology ;
			owl:someValuesFrom :TransparencyActivityTypology;
        ] ;
	 owl:disjointUnionOf ( :TransparencyPublicationActivity :TransparencyUpdateActivity ) ;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency> ; 
 .
 
