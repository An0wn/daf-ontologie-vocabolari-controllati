#### Classes #########

:Content rdf:type owl:Class ;
		rdfs:label "Contenuto" @it;
		rdfs:label "Content" @en;
		rdfs:subClassOf [  rdf:type owl:Restriction ;
                           owl:onProperty :publisher ;
                           owl:allValuesFrom covapit:PublicOrganization;
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty tiapit:issued ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange [ rdf:type rdfs:Datatype ;
                                             owl:unionOf ( xsd:date
                                                           xsd:dateTime
                                                         )
                                           ]
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty tiapit:modified ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange [ rdf:type rdfs:Datatype ;
                                             owl:unionOf ( xsd:date
                                                           xsd:dateTime
                                                         )
                                           ]
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :sourceURL ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:anyURI
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :hasSubject ;
                           owl:someValuesFrom :TransparencySubject;
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :comply ;
						   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :ObligationContent;
                         ]
						 ;
		rdfs:comment "Questa classe rappresenta i documenti, le informazioni e i dati oggetto di pubblicazione nei siti istituzionali delle pubbliche amministrazioni." @it;
		rdfs:comment "This class represents documents, informazions and data object of publication on the official website of the public administrations." @en;
		rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:ObligationContent rdf:type owl:Class ;
		rdfs:label "Contenuto dell'obbligo" @it;
		rdfs:label "Content of the obligation" @en;
		rdfs:subClassOf [  rdf:type owl:Restriction ;
                           owl:onProperty :accrualPeriodicity ;
						   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Frequency;
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :regulationReference ;
						   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange rdfs:Literal;
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :isObligationContentOf ;
						   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :TransparencyObligation;
                         ];
		rdfs:comment "Questa classe rappresenta il contenuto informativo riferito ad un obbligo di trasparenza oggetto di pubblicazione. Fa riferimento alla colonna 'contenuto dell'obbligo' dell'allegato alla guida ANAC. ";
		rdfs:comment "This class represents the informative content referred to a transparency obligation. It's related to the 'contenuto dell'obbligo' column of the ANAC guideline attachment."@en;
		rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:TransparencyObligation rdf:type owl:Class ;
		rdfs:label "Obbligo di trasparenza" @it;
		rdfs:label "Obligation of transparency" @en;
		rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :regulationReference ;
						   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange rdfs:Literal;
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :hasTransparencySubSection ;
						   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :TransparencyTitulus;
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :isAppliedTo ;
                           owl:allValuesFrom covapit:PublicOrganization;
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :affects ;
                           owl:allValuesFrom :TransparencySubject;
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :hasObligationContent ;
						   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :ObligationContent;
                         ];
		rdfs:comment "Questa classe rappresenta gli argomenti oggetto di obbligo di pubblicazione per la normativa sulla trasparenza. Fa riferimento alla colonna 'denominazione obbligo' dell'allegato alla guida ANAC."@it;
		rdfs:comment "This class represents the topic object of transparency obligation according to the transparency normative. It's related to the 'denominazione obbligo' column of the ANAC guideline attachment."@en;
		rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:TransparencySubject rdf:type owl:Class ;
		rdfs:label "Soggetto del contenuto della trasparenza" @it;
		rdfs:label "Subject of transparency contents" @en; 
		rdfs:subClassOf  l0:Entity,
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :hasContent ;
                           owl:someValuesFrom :Content;
                         ],
						 [ rdf:type owl:Restriction ;
                           owl:onProperty :isObjectOfObligation ;
                           owl:someValuesFrom :TransparencyObligation;
                         ];
		rdfs:comment "Questa classe rappresenta il soggetto a cui fa riferimento il contenuto da pubblicare."@it;
		rdfs:comment "This class represents the subject related to the content to publish"@en;
		rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

covapit:PublicOrganization rdf:type owl:Class ; 
	rdfs:isDefinedBy <https://w3id.org/italia/onto/COV/> ;
	rdfs:label "public organization"@en ,
               "Amministrazione pubblica"@it ,
               "organizzazione pubblica"@it ;
.	

:TransparencyTitulus rdf:type owl:Class ;
	rdfs:label "Denominazione sotto-sezione Amministrazione Trasparente"@it,
			   "Transparent administration sub-section denomination"@en;
	rdfs:comment "Questa classe la denominazione delle sotto-sezioni della sezione 'Amministrazione Trasparente' del sito istituzionale delle pubbliche amministrazioni. Le voci sono controllate dal vocabolario <https://w3id.org/italia/controlled-vocabulary/classifications-for-transparency/transparency-titulus>."@it,
				 "This class represents the subsections of the 'Transaprent Administrazion' section of the official website of the public administrations. The individuals are controlled by the vocabulary <https://w3id.org/italia/controlled-vocabulary/classifications-for-transparency/transparency-titulus>."@en;
	l0:controlledVocabulary <https://w3id.org/italia/controlled-vocabulary/classifications-for-transparency/transparency-titulus>;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/>;
.

l0:Entity rdf:type owl:Class ; 
	rdfs:isDefinedBy <https://w3id.org/italia/onto/l0/> .
	
:Frequency rdf:type owl:Class ;
                rdfs:comment "A rate at which something recurs. It is used to represent the frequency of update of published informative content. It assumes a specific set of values as defined by the controlled vocabulary available at http://publications.europa.eu/mdr/authority/frequency/index.html. The class is equivalent to the class defined by the Dublin Core standard"@en ,
                             "Numero delle volte che qualcosa si ripete. E' utlizzata per rappresentare la frequenza di aggiornamento del contenuto informativo pubblicato come specificato nella norma di riferimento. Essa assume un insieme finito di valori come definiti dal vocabolario controllato disponibile in http://publications.europa.eu/mdr/authority/frequency/index.html. La classe è equivalente alla classe dallo standard dublin core"@it ;
                rdfs:label "Frequency"@en ,
                           "Frequenza"@it ;
				rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/>;
				owl:equivalentClass <http://purl.org/dc/terms/Frequency> .

#### Object Properties ####

:comply rdf:type owl:ObjectProperty;
	rdfs:label "ottempera" @it;
	rdfs:label "comply" @en;
	rdfs:comment "Questa proprietà lega un contenuto informativo al contenuto del relativo obbligo di trasparenza."@it;
	rdfs:comment "This property links a informative content to the related transparency obligation content."@en;
	rdfs:range :ObligationContent;
	rdfs:domain :Content;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:hasSubject rdf:type owl:ObjectProperty;
	rdfs:label "ha argomento"@it;
	rdfs:label "has subject" @en;
	rdfs:comment "Questa proprietà lega un contenuto informativo all'argomento a cui si riferisce."@it;
	rdfs:comment "This property links an informative content to the related subject."@en;
	rdfs:range :TransparencySubject;
	rdfs:domain :Content;
	owl:inverseOf :hasContent;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:hasContent rdf:type owl:ObjectProperty;
	rdfs:label "ha contenuto informativo"@it;
	rdfs:label "has informative content" @en;
	rdfs:comment "Questa proprietà lega un soggetto (i.e. argomento o soggetto fisico)ai contenuto da pubblicare relativi."@it;
	rdfs:comment "This property links an informative content to the related subject."@en;
	rdfs:domain :TransparencySubject;
	rdfs:range :Content;
	owl:inverseOf :hasSubject;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.	

# http://purl.org/dc/terms/publisher 
:publisher rdf:type owl:ObjectProperty;
	rdfs:label "è pubblicato da" @it;
	rdfs:label "is published by" @en;
	rdfs:comment "Questa proprietà lega un contenuto alla pubblica amministrazione che lo pubblica."@it;
	rdfs:comment "This property links a content to the public administration that public it"@en;
	rdfs:range covapit:PublicOrganization;
	rdfs:domain :Content ;
	owl:inverseOf :publishes;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:publishes rdf:type owl:ObjectProperty;
	rdfs:label "pubblica" @it;
	rdfs:label "publishes" @en;
	rdfs:comment "Questa proprietà lega una pubblica amministrazione al contenuto che pubblica."@it;
	rdfs:comment "This property links a content to the public administration that public it"@en;
	rdfs:domain covapit:PublicOrganization;
	rdfs:range :Content ;
	owl:inverseOf :publisher;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:isObjectOfObligation rdf:type owl:ObjectProperty;
	rdfs:label "è oggetto di obbligo" @it;
	rdfs:label "is object of an obligation" @en;
	rdfs:comment "Questa proprietà lega un argomento ad un obbligo di trasparenza."@it;
	rdfs:comment "This property links a subject to a transparency obligation."@en;
	rdfs:range :TransparencyObligation;
	rdfs:domain :TransparencySubject;
	owl:inverseOf :affects;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:affects rdf:type owl:ObjectProperty;
	rdfs:label "è obbligo per" @it;
	rdfs:label "is obligation for" @en;
	rdfs:comment "Questa proprietà lega obbligo di trasparenza al soggetto che lo riguarda."@it;
	rdfs:comment "This property links a transparency obligation to the affected subjects."@en;
	rdfs:range :TransparencyObligation;
	rdfs:domain :TransparencySubject;
	owl:inverseOf :isObjectOfObligation;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:refersTo rdf:type owl:ObjectProperty;
	rdfs:label "si riferisce a" @it;
	rdfs:label "refers to" @en;
	rdfs:comment "Questa proprietà lega un argomento oggetto di obbligo di trasparenza all'entità, eventualmente già modellata in OntoPiA, a cui si riferisce.";
	rdfs:comment "This property links a subject to a related entity, eventually already modelled by OntoPiA."@en;
	rdfs:range owl:Thing;
	rdfs:domain :TransparencySubject;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:hasObligationContent rdf:type owl:ObjectProperty;
	rdfs:label "ha contenuto obbligo" @it;
	rdfs:label "has content of obligation" @en;
	rdfs:comment "Questa proprietà lega un obbligo di trasparenza ai suoi contenuti."@it;
	rdfs:comment "This property links a transparency obligation to its contents."@en;
	rdfs:range :ObligationContent;
	rdfs:domain :TransparencyObligation;
	owl:inverseOf :isObligationContentOf;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:isObligationContentOf rdf:type owl:ObjectProperty;
	rdfs:label "è contenuto obbligo di" @it;
	rdfs:label "if obligation content of" @en;
	rdfs:comment "Questa proprietà lega un obbligo di trasparenza ai suoi contenuti."@it;
	rdfs:comment "This property links a transparency obligation to its contents."@en;
	rdfs:domain :ObligationContent;
	rdfs:range :TransparencyObligation;
	owl:inverseOf :hasObligationContent;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:isAppliedTo rdf:type owl:ObjectProperty;
	rdfs:label "si applica a" @it;
	rdfs:label "is applied to" @en;
	rdfs:comment "Questa proprietà lega un obbligo di trasparenza alla pubblica amministrazione a cui si applica."@it;
	rdfs:comment "This property links a transparency obligation to a public administration to whom is applied."@en;
	rdfs:range covapit:PublicOrganization;
	rdfs:domain :TransparencyObligation;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.

:accrualPeriodicity rdf:type owl:ObjectProperty;
	rdfs:label "accrual periodicity"@en ,
               "frequenza di aggiornamento"@it;
	rdfs:comment "Questa proprietà lega un obbligo di trasparenza alla frequenza con cui va aggiornato il contenuto. vedi dcatapit."@it;
	rdfs:comment "This property links a transparency obligation to the update frequency of the content."@en;
	rdfs:range :Frequency;
	rdfs:domain :ObligationContent;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
	owl:equivalentProperty <http://purl.org/dc/terms/accrualPeriodicity>;
.

:hasTransparencySubSection rdf:type owl:ObjectProperty;
	rdfs:label "ha sotto-sezione obbligo" @it;
	rdfs:label "has transparency sub section" @en;
	rdfs:comment "Questa proprietà lega un obbligo di trasparenza alla sotto-sezione di 'Amministrazione Trasparente' in cui devono essere pubblicati i suoi contenuti. Le sotto-sezioni sono controllate della tassonomia https://w3id.org/italia/controlled-vocabulary/classifications-for-transparency/transparency-titulus"@it;
	rdfs:comment "This property links a transparency obligation to the sub-section of the 'Transparent Administration' section where the contents must be published. The values are controlled by the taxonomy  https://w3id.org/italia/controlled-vocabulary/classifications-for-transparency/transparency-titulus "@en;
	rdfs:range :TransparencyTitulus ;
	rdfs:domain :TransparencyObligation ;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency/> ;
.



