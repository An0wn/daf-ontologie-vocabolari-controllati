###  https://w3id.org/italia/onto/Transparency/TransparencyResource
:TransparencyResource a owl:Class ;
	rdfs:label "Transparency resource"@en, "Risorsa di trasparenza"@it;
	rdfs:subClassOf <https://w3id.org/italia/onto/l0/Object> ,
	[ a owl:Restriction ;
		owl:onProperty <https://w3id.org/italia/onto/l0/name> ;
		owl:someValuesFrom rdfs:Literal
	] ,
	[ a owl:Restriction ;
		owl:onProperty <https://w3id.org/italia/onto/TI/modified> ;
		owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		owl:onDataRange [ a rdfs:Datatype ;
			owl:unionOf ( 
				xsd:date
				xsd:dateTime
				xsd:time
			)
		]
	] ,
	[ a owl:Restriction ;
		owl:onProperty :refersToTransparencyEntity ;
		owl:someValuesFrom [ a owl:Class ;
			owl:unionOf ( 
			<https://w3id.org/italia/onto/l0/Activity>
			<https://w3id.org/italia/onto/l0/Object>
			)
		]
	],
	[ a owl:Restriction ;
		owl:onProperty :accessURL ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onDataRange xsd:anyURI ; 
    ] ;
	rdfs:comment "Questa classe rappresenta i documenti, le informazioni e i dati oggetto di pubblicazione nei siti istituzionali delle pubbliche amministrazioni." @it;
	rdfs:comment "This class represents documents, informazions and data object of publication on the official website of the public administrations." @en;
