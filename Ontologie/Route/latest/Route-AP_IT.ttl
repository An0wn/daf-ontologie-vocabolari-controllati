###  https://w3id.org/italia/onto/Route/crosses
:crosses rdf:type owl:ObjectProperty ;
         owl:inverseOf :isCrossedBy ;
         rdfs:domain :Route ;
         rdfs:range :GeospatialPoint ;
         owl:propertyChainAxiom ( :enumeratesPoint
                                  :hasGeospatialPoint
                                ) ;
         rdfs:label "La relazione che permette di asserire che un percorso attraversa un certo punto geospaziale."@it ,
                    "The relation between a route and a geospatial point. I.e. a route 'crosses' a certain geospatial point."@en ,
                    "attraversa"@it ,
                    "crosses"@en .


###  https://w3id.org/italia/onto/Route/enumeratesPoint
:enumeratesPoint rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isEnumeratedByRoute ;
                 rdfs:domain :Route ;
                 rdfs:range :GeospatialPointItem ;
                 rdfs:comment "La relazione generica tra un percorso e una relazione n-aria che rappresenta un punto in una sequenza."@it ,
                              "The generic relation between a route and a point sequencing in route."@en ;
                 rdfs:label "enumera punti"@it ,
