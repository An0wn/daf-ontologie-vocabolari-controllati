###  https://w3id.org/italia/onto/Route/GeospatialPoint
:GeospatialPoint rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                              owl:onProperty <https://w3id.org/italia/onto/CLV/hasGeometryType> ;
                                                              owl:hasValue <https://w3id.org/italia/onto/CLV/Point>
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <https://w3id.org/italia/onto/CLV/lat> ;
                                                              owl:cardinality "1"^^xsd:nonNegativeInteger
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <https://w3id.org/italia/onto/CLV/long> ;
                                                              owl:cardinality "1"^^xsd:nonNegativeInteger
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf <https://w3id.org/italia/onto/CLV/Geometry> .


###  https://w3id.org/italia/onto/Route/GeospatialPointItem
:GeospatialPointItem rdf:type owl:Class ;
                     rdfs:subClassOf l0:Description ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasGeospatialPoint ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :GeospatialPoint
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasNextPoint ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :GeospatialPoint
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasPreviousPoint ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :GeospatialPoint
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :ordering ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onDataRange xsd:int
                                     ] .


###  https://w3id.org/italia/onto/Route/Route
:Route rdf:type owl:Class ;
       rdfs:subClassOf <https://w3id.org/italia/onto/CLV/Geometry> ,
                       l0:Sequence ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :enumeratesPoint ;
                         owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                         owl:onClass :GeospatialPointItem
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasEndPoint ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :GeospatialPointItem
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasLength ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass <https://w3id.org/italia/onto/MU/Value>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasStartPoint ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :GeospatialPointItem
                       ] .


