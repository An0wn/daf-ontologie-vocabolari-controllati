###  https://w3id.org/italia/onto/AccessCondition/hasAccessCondition

<https://w3id.org/italia/onto/AccessCondition/hasAccessCondition> rdf:type owl:ObjectProperty ;
                                                                  
                                                                  rdfs:label "has access condition"@en ,
                                                                             "ha condizione d'accesso"@it ;
                                                                  
                                                                  rdfs:comment "Property that links a thing of the modelled domain to access conditions that may apply."@en ;
                                                                  
                                                                  owl:versionInfo "stable"@en ;
                                                                  
                                                                  rdfs:comment "Proprietà che collega qualcosa del dominio modellato alle condizioni di accesso che si potrebbero applicare a quella cosa. Per esempio, si potrebbe utilizzare la proprietà per collegare un punto di interesse, un luogo della cultura, un evento pubblico, un ufficio della pubblica amministrazione, un servizio pubblico a condizioni per accedervi"@it ;
                                                                  
                                                                  owl:versionInfo "stabile"@it ;
                                                                  
                                                                  rdfs:domain owl:Thing ;
                                                                  
                                                                  rdfs:isDefinedBy <https://w3id.org/italia/onto/AccessCondition> ;
                                                                  
                                                                  rdfs:range <https://w3id.org/italia/onto/AccessCondition/AccessCondition> .



###  https://w3id.org/italia/onto/AccessCondition/hasAdmissionType

<https://w3id.org/italia/onto/AccessCondition/hasAdmissionType> rdf:type owl:ObjectProperty ;
                                                                
                                                                rdfs:label "has admission type"@en ,
                                                                           "ha tipo di apertura"@it ;
                                                                
                                                                rdfs:comment "Property that links the opening hours specificatin to the admission type (weekly, daily, etc.)"@en ;
                                                                
                                                                owl:versionInfo "stable"@en ;
                                                                
                                                                rdfs:comment "Proprietà che lega gli orari di apertura al tipo di apertura (settimanale, giornaliera, ecc.)"@it ;
                                                                
                                                                owl:versionInfo "stabile"@it ;
                                                                
                                                                rdfs:isDefinedBy <https://w3id.org/italia/onto/AccessCondition> ;
                                                                
                                                                rdfs:range <https://w3id.org/italia/onto/AccessCondition/AdmissionType> ;
                                                                
                                                                rdfs:domain <https://w3id.org/italia/onto/AccessCondition/OpeningHoursSpecification> ;
                                                                
                                                                owl:inverseOf <https://w3id.org/italia/onto/AccessCondition/isAdmissionTypeOf> .



