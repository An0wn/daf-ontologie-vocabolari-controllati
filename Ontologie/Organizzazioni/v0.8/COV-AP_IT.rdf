    <rdfs:label xml:lang="it">nome legale</rdfs:label>
    <rdfs:label xml:lang="en">preferred label</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="it">Questa proprietà rappresenta il nome primario o il nome legale dell'organizzazione oppure può essere utilizzata per rappresentare l'etichetta primaria per il ruolo.</rdfs:comment>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:comment xml:lang="en">This is the primary or legal name for the organization or it can be used to indicate the primary label of a role.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:nodeID="ub50bL758C24"/>
