    <owl:versionInfo xml:lang="en">Version 0.8 - 25  October 2017 - updated refactorisation changes to Social Media ontology used in this one.</owl:versionInfo>
    <owl:imports rdf:resource="http://dati.gov.it/onto/tiapit/"/>
