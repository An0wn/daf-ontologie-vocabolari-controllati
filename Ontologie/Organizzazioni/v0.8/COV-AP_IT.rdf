    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="it">nome alternativo</rdfs:label>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:label xml:lang="en">alternative name</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/covapit#officeIdentifier">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="it">Questa proprietà rappresenta il codice univoco dell'ufficio dell'organizzazione.</rdfs:comment>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/covapit#SupportUnit"/>
