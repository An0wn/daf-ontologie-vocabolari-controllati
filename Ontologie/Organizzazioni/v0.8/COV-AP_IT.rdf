    <rdfs:subClassOf rdf:nodeID="ub50bL1215C31"/>
    <rdfs:subClassOf rdf:nodeID="ub50bL1190C31"/>
    <rdfs:subClassOf rdf:nodeID="ub50bL1151C31"/>
    <rdfs:subClassOf rdf:nodeID="ub50bL1155C31"/>
  </rdf:Description>
