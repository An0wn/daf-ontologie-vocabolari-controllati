    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <owl:versionInfo xml:lang="it">Questa proprietà rappresenta un nome alternativo dell'organizzazione (e.g., un nome colloquiale).</owl:versionInfo>
    <rdfs:comment xml:lang="en">This property represents an alternative name for the organization such as a colloquial name.</rdfs:comment>
