  <rdf:Description rdf:about="http://dati.gov.it/onto/covapit#Group">
    <rdfs:comment xml:lang="it">Questa classe rappresenta un gruppo ossia una collezione di agenti individuatli. Il concetto è volutamente ampio per poter rappresentare gruppi informale ma anche gruppi formati ad-hoc, comunità, ecc.</rdfs:comment>
    <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/covapit/"/>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/l0/Agent"/>
    <prov:wasDerivedFrom rdf:resource="http://xmlns.com/foaf/0.1/Group"/>
    <rdfs:comment xml:lang="en">This class represents a group, a collection of individual agents. This concept is intentionally quite broad, covering informal and ad-hoc groups, long-lived communities, organizational groups within a workplace, etc.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://dati.gov.it/onto/covapit#Organization"/>
    <rdfs:label xml:lang="en">Group</rdfs:label>
    <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    <rdfs:label xml:lang="it">Gruppo</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
