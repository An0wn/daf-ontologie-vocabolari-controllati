        <IRI>Email</IRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Email</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>Email</IRI>
        <Literal xml:lang="it" datatypeIRI="&rdf;PlainLiteral">Email</Literal>
    </AnnotationAssertion>
