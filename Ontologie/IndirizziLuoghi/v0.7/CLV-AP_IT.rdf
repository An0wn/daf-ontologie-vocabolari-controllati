        <rdfs:comment xml:lang="en">Date of last update used for both internal and external number as well as for the address.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
        <rdfs:label xml:lang="it">data di ultimo aggiornamento</rdfs:label>
        <rdfs:label xml:lang="en">date of last update</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:DatatypeProperty>
    


    <!-- http://dati.gov.it/onto/clvapit#peculiarity -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/clvapit#peculiarity">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#ExternalAddressNumber"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Valore utilizzato in presenza di uno specifico metodo di classificazione della numerazione civica, validata dall’Istat (es. ROSSO e NERO); Potrebbe forse essere utilizzato anche per indicare A, B , bis che si riferiscono ai numeri civici di accesso esterno.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
        <rdfs:label xml:lang="en">peculiarity</rdfs:label>
        <rdfs:label xml:lang="it">specificità</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria - potrebbe essere che il codominio Literal non sia appropriato rispetto ai valori assunti dalla proprietà. Potrebbe essere necessario creare un&apos;object property e creare una classe Peculiarity che assume valori predefiniti di una classificazione definita da ISTAT. Questo è da verificare con esempi di dati dell&apos;ANNCSU</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:DatatypeProperty>
    


    <!-- http://dati.gov.it/onto/clvapit#postalCode -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/clvapit#postalCode">
