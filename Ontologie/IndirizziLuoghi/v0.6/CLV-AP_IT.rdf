    <rdfs:label xml:lang="it">Numero Civico</rdfs:label>
    <rdfs:subClassOf rdf:nodeID="ub237bL1637C40"/>
    <rdfs:subClassOf rdf:nodeID="ub237bL1629C40"/>
    <rdfs:subClassOf rdf:nodeID="ub237bL1633C40"/>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/clvapit#AddressNumber"/>
