      "rdfs:comment": [
        {
          "@language": "it",
          "@value": "Qualsiasi cosa reale, possibile o immaginaria."
        },
        {
          "@language": "en",
          "@value": "Anything: real, possible, or imaginary, which some modeller wants to talk about for some purpose."
        }
