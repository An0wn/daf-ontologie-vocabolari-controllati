    <rdfs:subClassOf rdf:nodeID="ub5bL460C30"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/TI/TimeIndexedEvent"/>
    <rdfs:subClassOf rdf:nodeID="ub5bL464C30"/>
    <rdfs:label xml:lang="it">Evento Pubblico</rdfs:label>
    <rdfs:comment xml:lang="en">This class represents a public event (e.g., a football match, a  conference, a book presentation, etc.). A public event is defined as an event that takes places in a given place and that occurs at a certain time, organised/run/promoted by one or more agents with a specific purpose and in the interest of a general public. There exist different types of public events depending on their size and content.</rdfs:comment>
    <rdfs:subClassOf rdf:nodeID="ub5bL468C30"/>
    <rdfs:subClassOf rdf:nodeID="ub5bL438C30"/>
    <rdfs:subClassOf rdf:nodeID="ub5bL451C30"/>
